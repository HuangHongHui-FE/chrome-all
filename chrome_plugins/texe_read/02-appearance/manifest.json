{
    "manifest_version": 2,
    "name": "One-click Kittens",
    "description": "啥也不是",
    "version": "1.0",
    "permissions": [
        "contextMenus",
        "notifications"
    ],
    "icons": {
        "16": "icon-bitty.png",
        "48": "icon-small.png",
        "128": "icon-large.png"
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "黄的人",
        "default_popup": "popup.html"
    }
}